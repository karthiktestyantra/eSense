  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "allure-commandline": "^2.18.1",
    "cypress": "^10.8.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^3.0.2",
    "cypress-mochawesome-reporter": "^3.1.0",
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.3",
    "moment": "^2.29.4"
  }
}